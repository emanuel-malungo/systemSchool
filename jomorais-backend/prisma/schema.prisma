generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id                BigInt       @id @default(autoincrement()) @db.UnsignedBigInt
  name              String
  username          String       @unique(map: "users_username_unique") @default("root")
  email             String       @unique(map: "users_email_unique")
  email_verified_at DateTime?    @db.Timestamp(0)
  password          String
  tipo              Int          @default(1)
  remember_token    String?      @db.VarChar(100)
  created_at        DateTime?    @db.Timestamp(0)
  updated_at        DateTime?    @db.Timestamp(0)
  foto              String       @default("img_avatar1.png")
  materias          materias[]
  tb_alunos         tb_alunos[]
  tb_docente        tb_docente[]

  @@map("users")
}

model tb_tipos_utilizador {
  codigo              Int                   @id @default(autoincrement()) @map("Codigo") @db.UnsignedInt
  designacao          String                @map("Designacao") @db.VarChar(45)
