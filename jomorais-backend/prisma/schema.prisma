  tb_turmas        tb_turmas       @relation(fields: [codigoTurma], references: [codigo], onDelete: NoAction, onUpdate: NoAction, map: "FK_permissao_turma_utilizador_tuma")

  @@index([codigoTurma], map: "FK_permissao_turma_utilizador_tuma")
  @@index([codigoUtilizador], map: "FK_permissao_turma_utilizador_Utilizador")
}

