  status            String          @default("desactivo") @map("status") @db.VarChar(45)
  data_Inicio       DateTime        @default(dbgenerated("'0000-00-00'")) @map("Data_Inicio") @db.Date
  data_fin          DateTime        @default(dbgenerated("'0000-00-00'")) @map("Data_fin") @db.Date
